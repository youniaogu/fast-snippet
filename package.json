{
  "name": "fast-snippet",
  "displayName": "fast-snippet",
  "description": "a vscode extensions for fast write snippet to clipboard",
  "keywords": ["fast", "snippet", "copy", "paste", "template"],
  "author": "youniaogu",
  "publisher": "youniaogu",
  "icon": "static/logo.png",
  "bugs": "https://github.com/youniaogu/fast-snippet/issues",
  "repository": "https://github.com/youniaogu/fast-snippet",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fast-snippet.start"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fast-snippet.start",
        "title": "Fast Snippet(Input name and select template)"
      }
    ],
    "configuration": [
			{
        "type": "object",
        "title": "Fast-snippet configuration",
        "properties": {
					"fastSnippet.template": {
						"type": "object",
            "description": "Code template",
            "default": {}
          }
        }
			}
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --devtool nosources-source-map --info-verbosity verbose --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.51.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "eslint": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0",
    "ts-loader": "^8.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "license": "MIT"
}